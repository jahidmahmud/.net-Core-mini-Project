@model IEnumerable<PracticeCore.Models.Book>
@{ 
    ViewData["title1"] = "Book List";
}
<div class="container">
    <h1>Books</h1>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <partial name="_bookThumbnail" model="item"/>
            </div>
        }

    </div>
</div>

